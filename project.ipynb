   ],
   "source": [
    "import random\n",
    "import string\n",
    "length=6\n",
    "otp=\"\"\n",
    "character=string.ascii_letters+string.digits\n",
    "for i in range(length):\n",
    "    otp=otp+random.choice(character)\n",
    "print(\"OTP is\",otp)"
   ]
  }
